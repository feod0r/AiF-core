  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-backend": "cd backend_py && python -m uvicorn src.app.create_app:app --reload --host 0.0.0.0 --port 8000",
    "start-full": "concurrently \"npm run start-backend\" \"npm run start\"",
    "install-backend": "cd backend_py && pip install -r requirements.txt",
    "setup-backend": "cd backend_py && python -m pip install poetry && poetry install"
